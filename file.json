{
    "family": "fargate", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "fargate-app", 
            "image": "921759041822.dkr.ecr.us-east-1.amazonaws.com/webapp:webapp_7", 
            "portMappings": [
                {
                    "containerPort": 8080, 
                    "hostPort": 5000, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true, 
            "entryPoint": [
                "sh",
		"-c"
            ], 
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
