  "taskRoleArn": "arn:aws:iam::921759041822:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::921759041822:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "javaapp",
      "image": "921759041822.dkr.ecr.us-east-1.amazonaws.com/webapp:webapp_8",
      "essential": true
    }
      },
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8080
        }
      ]
    }
  ]
}
