{
  "family": "webapp",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "taskRoleArn": "arn:aws:iam::921759041822:role/ecs_role_Task",
  "executionRoleArn": "arn:aws:iam::921759041822:role/ecs_role_Task",
  "containerDefinitions": [
    {
      "name": "javaapp",
      "image": "921759041822.dkr.ecr.us-east-1.amazonaws.com/webapp:webapp_7",
      "essential": true,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "awslogs-group": "/ecs/awslogs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
