{
  "family": "fargate1",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "networkMode": "awsvpc",
  "taskRoleArn": "arn:aws:iam::921759041822:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::921759041822:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "javaapp",
      "image": "921759041822.dkr.ecr.us-east-1.amazonaws.com/webapp:webapp_7",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/workshop",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8080
        }
      ]
    }
  ]
}
